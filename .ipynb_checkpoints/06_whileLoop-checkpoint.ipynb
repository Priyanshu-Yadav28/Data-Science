    "# sum up the number till some  point\n",
    "num = int (input(\"Enter the number till you want to find the sum\"))\n",
    "\n",
    "start = 1\n",
    "sum = 0\n",
    "\n",
    "while start <= num :\n",
    "    sum = sum + start\n",
    "    start = start + 1\n",
    "\n",
    "print(sum)"
